            <Font size="15.0" />
         </font>
      </TextField>
      <Label fx:id="errorNotification" alignment="CENTER" layoutX="58.0" layoutY="81.0" prefHeight="17.0" prefWidth="296.0" textAlignment="CENTER" textFill="#ec0000" wrapText="true" />
      <Label fx:id="successNotification" alignment="CENTER" layoutX="58.0" layoutY="81.0" prefHeight="17.0" prefWidth="296.0" textAlignment="CENTER" textFill="#02ec35" wrapText="true" />
   </children>
